              "application/json": "{\n  \"myRoomsScores\": [\n    {\n      \"id\": 1,\n      \"name\": \"Sala 1\",\n      \"users\": [\n        {\n          \"score\": 200,\n          \"id\": 1,\n          \"name\": \"usuario1\",\n          \"picture\": \"image/imagem.jpg\"\n        }\n      ]\n    }\n  ],\n  \"roomsScores\": [\n    {\n      \"roomId\": 1,\n      \"score\": 177,\n      \"points\": 260\n    }\n  ],\n  \"questionsRoomScores\": [\n    {\"id\": 1,\n      \"questions\": [\n        {\n          \"id\": 2,\n          \"order\": 3,\n          \"points\": 80,\n          \"description\": \"Qual o resultado de 1 x 1 ?\",\n          \"score\": 37\n        }\n      ]\n    }\n  ],\n  \"allUserScores\": [\n    {\n      \"roomId\": 1,\n      \"userId\": 2,\n      \"score\": \"186\"\n    }, {\n      \"roomId\": 1,\n      \"userId\": 1,\n      \"score\": \"177\"\n    }\n  ]\n}"
            }
          },
          "401": {
            "description": "Não autorizado."
          },
          "403": {
            "description": "Acesso proibido."
          }
        }
